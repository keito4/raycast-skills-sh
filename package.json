{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "skills",
  "title": "Skills",
  "description": "Browse and search AI agent skills from Skills",
  "icon": "icon.png",
  "author": "keito4",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "browse-skills",
      "title": "Browse Skills",
      "description": "Browse AI agent skills ranking by total installs",
      "mode": "view"
    },
    {
      "name": "search-skills",
      "title": "Search Skills",
      "description": "Search for AI agent skills",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "1.90.0",
    "@raycast/utils": "1.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.4",
    "eslint": "^9.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
