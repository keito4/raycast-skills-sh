{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "skills-sh",
  "title": "Skills.sh",
  "description": "Browse and search AI agent skills from skills.sh",
  "icon": "icon.png",
  "author": "keito4",
  "categories": ["Developer Tools"],
  "license": "MIT",
  "commands": [
    {
      "name": "browse-skills",
      "title": "Browse Skills",
      "description": "Browse AI agent skills ranking by total installs",
      "mode": "view"
    },
    {
      "name": "search-skills",
      "title": "Search Skills",
      "description": "Search for AI agent skills",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.0.0",
    "@raycast/utils": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "22.13.10",
    "@types/react": "^19.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
